{
  "name": "@parakeet/api",
  "version": "1.0.0",
  "description": "parakeet api code",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:check": "eslint \"**/*.*\" --report-unused-disable-directives",
    "eslint:fix": "eslint \"**/*.*\" --quiet --fix",
    "prettier:check": "prettier \"**/*.*\" --check --ignore-path ./.prettierignore",
    "prettier:fix": "prettier \"**/*.*\" --write --ignore-path ./.prettierignore",
    "test": "jest src",
    "test:watch": "jest --watchAll",
    "test:cov": "jest src --coverage",
    "test:cov:watch": "jest --coverage --watchAll",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "nodemon dist/app/entry.js",
    "start": "node dist/app/entry.js"
  },
  "lint-staged": {
    "*.*": [
      "eslint --quiet --fix --ignore-path .eslintignore",
      "prettier --write --ignore-path ./.prettierignore"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.0.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cronstrue": "^1.113.0",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.0.0",
    "swagger-ui-express": "^4.1.6",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.14",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.1.3"
  }
}
