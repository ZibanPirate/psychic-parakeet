{
  "name": "@parakeet/web",
  "version": "1.0.0",
  "description": "frontend code for parakeet",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "yarn clear && cross-env NODE_ENV=development webpack serve --config ./webpack.config.js",
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:check": "eslint \"**/*.*\" --report-unused-disable-directives",
    "eslint:fix": "eslint \"**/*.*\" --quiet --fix",
    "prettier:check": "prettier \"**/*.*\" --check --ignore-path ./.prettierignore",
    "prettier:fix": "prettier \"**/*.*\" --write --ignore-path ./.prettierignore",
    "test": "jest src",
    "test:watch": "jest --watchAll src",
    "test:cov": "jest --coverage src",
    "test:cov:watch": "jest --coverage --watchAll src",
    "build": "yarn clear && cross-env NODE_ENV=production webpack --config ./webpack.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.*": [
      "eslint --quiet --fix --ignore-path .eslintignore",
      "prettier --write --ignore-path ./.prettierignore"
    ]
  },
  "devDependencies": {
    "@parakeet/api": "1.0.0",
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@prettier/plugin-pug": "^1.15.2",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "base64-inline-loader": "^1.1.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-unicorn": "^32.0.1",
    "file-loader": "^6.2.0",
    "fontsource-roboto": "^4.0.0",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.2.0",
    "precss": "^4.0.0",
    "prettier": "^2.3.0",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12",
    "sass-loader": "^11.1.0",
    "source-map-loader": "^2.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
